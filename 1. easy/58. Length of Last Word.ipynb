   "source": [
    "Вернуть длину последнего слова"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "6f6f860a",
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "\n",
    "class Solution:\n",
    "    def lengthOfLastWord(self, s: str) -> int:\n",
    "        return len(re.sub('[!.,?]', '', s).split()[-1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "60fa9337",
   "metadata": {},
   "outputs": [
